alias home="cd ~ && ls"
alias up="cd .. && ls"
alias etp="cd /etc/portage && ls"
alias epu="cd /etc/portage/package.use && ls"
alias epm="cd /etc/portage/package.mask && ls"
alias conf"cd ${XDG_CONFIG_HOME} && ls"
alias trp='trash-put'
alias tre='trash-empty'
alias trl='trash-list'
alias trr='trash-restore'
alias trm='trash-rm'

# Copy output of last command
alias c='"$(history | tail -2 | sed "s/  [0-9]*  //;2d")" > /tmp/cmdoutput && cat /tmp/cmdoutput | xsel -b && notify-send "Terminal" "Ouput Copied"'


